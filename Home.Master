                transition: 0.3s ease all;
                /*border-left: 1px solid #fff;
                    width: 100%;
                    margin-left: auto;
                    padding: 0;*/
            }

            .menu__nesting-redes > li:hover {
                border-bottom: none;
            }

            .menu__nesting-redes > li > a {
                margin: 0px;
